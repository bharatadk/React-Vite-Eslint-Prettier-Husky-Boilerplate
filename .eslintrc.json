{
  "env": {
  "browser": true,
  "es2021": true
},
"extends": ["airbnb", "plugin:prettier/recommended"],
"parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
},
  "rules": 
  {
    "react/react-in-jsx-scope": "off"

  },
  "settings":{
    "react":{
      "version":"detect"
    }
  }

}
